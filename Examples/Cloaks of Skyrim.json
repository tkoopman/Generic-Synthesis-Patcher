/*
  Forwards Outfit changes made by Cloaks of Skyrim to make sure not overwritten.
  Also sets survival warmth on some cloaks.
*/

[
  {
    "types": "Outfit",
    "Masters": "Skyrim.esm",
    "ForwardOptions": "SelfMasterOnly",
    "Forward": { "Cloaks.esp": "Items" }
  },
  {
    "types": "Outfit",
    "Masters": "Dawnguard.esm",
    "ForwardOptions": "SelfMasterOnly",
    "Forward": { "Cloaks - Dawnguard.esp": "Items" }
  },
  {
    "types": "Armor",
    "Masters": "Cloaks.esp",
    "Matches": { "Keywords": [ "ArmorMaterialLeather", "ArmorMaterialHide" ] },
    "Fill": { "Keywords": "Survival_ArmorWarm" }
  }
]