[
  {
    "Types": "Book",
    "SingleMatch": true,
    "Rules": [
      {
        "Matches": { "InventoryArt": "+/^(?!.*(?:(?:Book)|(?:Journal)|(?:Tome))).*((?:Note)|(?:Scroll)|(?:Paper)|(?:Map)|(?:Recipe)).*$/" },
        "Fill": { "Type": "NoteOrScroll" }
      },
      {
        "Matches": { "InventoryArt": "-/^(?!.*(?:(?:Book)|(?:Journal)|(?:Tome))).*((?:Note)|(?:Scroll)|(?:Paper)|(?:Map)|(?:Recipe)).*$/" },
        "Fill": { "Type": "BookOrTome" }
      }
    ]
  }
]